var* arr = input[1..8];

fn sort(array, length) {
    var i = 0;
    while (i < length) {
        var j = i + 1;
        while (j < length) {
            if (array[i] > array[j]) {
                var x = array[j];
                array[j] = array[i];
                array[i] = x;
            }
            j++;
        }
        i++;
    }
}

sort(arr, 8);

output[1..8] = arr;