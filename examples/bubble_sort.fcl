var* array = input[1..8];
var length = 8;

var i = 0;
while (i < length) {
    var j = i + 1;
    while (j < length) {
        var a = array[i];
        var b = array[j];
        if (a > b) {
            array[j] = a;
            array[i] = b;
        }
        j++;
    }
    i++;
}

output[1..8] = array;