var* arr = input[1..8];

fn sort(array, length) {
    var i = 0;
    while (i < length) {
        var j = i + 1;
        while (j < length) {
            var a = array[i];
            var b = array[j];
            if (a > b) {
                array[j] = a;
                array[i] = b;
            }
            j++;
        }
        i++;
    }
}

sort(arr, 8);

output[1..8] = arr;