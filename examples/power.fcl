fn power(a, n) {
    var result = 1;
    while (n > 0) {
        if (n % 2 == 0) {
            n = n / 2;
            a = a * a;
        } else {
            n--;
            result = result * a;

            n = n / 2;
            a = a * a;
        }
    }
} ret result;

output[1] = power(input[1], input[2]);
